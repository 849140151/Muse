<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Muse.UI.ViewModel"
                    xmlns:view="clr-namespace:Muse.UI.View">


    <DataTemplate DataType="{x:Type vm:SongListVM}">
        <view:SongList />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PlayBarVM}">
        <view:PlayBar />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LyricVM}">
        <view:Lyric />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LyricManagerVM}">
        <view:LyricManager />
    </DataTemplate>
    
</ResourceDictionary>